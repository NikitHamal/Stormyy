{
    "version": 2,
    "functions": {
        "api/crawl-images.py": {
            "memory": 1024,
            "maxDuration": 10
        }
    },
    "routes": [
        {
            "src": "/api/crawl-images",
            "methods": ["POST", "OPTIONS"],
            "headers": {
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Headers": "Content-Type, Accept",
                "Access-Control-Allow-Methods": "POST, OPTIONS"
            },
            "dest": "/api/crawl-images.py"
        }
    ]
} 